<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% set css %}
      {% include "css/main.css" %}
    {% endset %}
    <style>
      {{ css | cssmin | safe }}
    </style>
    <title>{{ title }}</title>
  </head>
  <body {% if bodyClass %}class="{{ bodyClass }}"{% endif %}>
    <div class="container">
      {% include "header.njk" %}
      {{ content | safe }}
    </div>
  </body>
</html>



